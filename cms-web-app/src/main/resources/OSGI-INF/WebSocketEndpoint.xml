<?xml version="1.0" encoding="UTF-8"?>
<component name="CMSWebSocketEndpoint" immediate="true">
    <implementation class="media.dee.dcms.webapp.cms.internal.WebSocketEndpoint" activate="activate" deactivate="deactivate"/>
    <property name="service.description" value="DeeCMS admin web app" />
    <property name="service.vendor" value="d.CMS" />

    <reference name="WebSocketService"
               interface="media.dee.dcms.websocket.WebSocketService"
               cardinality="0..n"
               policy="dynamic"
               bind="bindWebSocketService"
               unbind="unbindWebSocketService"
    />


    <reference name="log"
               interface="org.osgi.service.log.LogService"
               cardinality="0..n"
               policy="dynamic"
               bind="bindLong"
               unbind="unbindLong"
    />

    <service>
        <provide interface="media.dee.dcms.websocket.WebSocketEndpoint"/>
    </service>
</component>